<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item 
        android:state_pressed="false">
        <shape>
            <size 
                android:width="15dp"
                android:height="15dp"/>
            <!-- 33cccccc-->
